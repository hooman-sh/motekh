  "main": "app.js",
  "scripts": {
    "start": "concurrently \"npm run start --prefix public/landing\" \"cd ./public/profile; sudo grunt watch\" \"node app.js\" ",
    "installFront": "cd public/profile;npm install ;cd ../landing ;npm install ;cd ../profile;bower install;grunt;",
    "installFrontWindows": "cd public/profile && npm install && cd ../landing && npm install && cd ../profile && bower install && grunt",
    "packFront": "cd ./public/profile; sudo grunt; npm run build --prefix ../landing",
    "watch": "concurrently \"npm run start --prefix public/landing\" \"cd ./public/profile; sudo grunt watch\" \"node app.js\" ",
    "packFrontWindows": "cd ./public/profile && grunt && npm run buildWindows --prefix ../landing",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
